{
  "name": "tilelive-decorator",
  "version": "3.0.0",
  "description": "Load vector tiles from another source and decorate them with properties from redis.",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "pretest": "eslint index.js test/index.js",
    "test": "tape test/*.js",
    "coverage": "nyc --reporter html tape test/*.js && open coverage/index.html"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "benchmark": "^2.1.3",
    "eslint": "^1.10.3",
    "eslint-config-mourner": "^1.0.1",
    "nyc": "^5.5.0",
    "pbf": "^1.3.5",
    "tape": "^4.2.0",
    "tilelive-s3": "^6.1.0",
    "vector-tile": "^1.1.3"
  },
  "dependencies": {
    "@mapbox/tile-decorator": "mapbox/tile-decorator#merge-order",
    "lru-cache": "^4.0.0",
    "redis": "^2.0.1",
    "tilelive": "5.12.x"
  },
  "eslintConfig": {
    "extends": "mourner",
    "rules": {
      "space-before-function-paren": [
        2,
        "never"
      ],
      "no-new": 0,
      "camelcase": 0
    }
  }
}
